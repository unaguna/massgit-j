{
  "name": "massgit",
  "description": "massgit -- a tool that enables you to perform the same operation on multiple Git repositories with a single command execution.",
  "usages": [
    "%s [<options>] <subcommand> [<args>]",
    "%s (--version | --help)"
  ],
  "options": [
    {
      "names": ["--marker", "-m"],
      "type": "String",
      "dest": "marker_condition",
      "description": "Specify target repositories by marker. You can specify boolean expression such as 'marker1 or marker2'."
    },
    {
      "names": ["--rep-suffix"],
      "type": "String",
      "dest": "rep_suffix",
      "description": "The suffix of repository name in output. Default is ': ' in almost every subcommand."
    },
    {
      "names": ["--version"],
      "type": null,
      "dest": null,
      "description": "Show version number of massgit"
    }
  ],
  "subcommands": [
    {
      "name": "mg-clone",
      "subdesc": "Clone repositories written in repos.json.",
      "usages": [
        "%s [<options>] %s",
        "%s %s (--version | --help)"
      ],
      "options": [
      ]
    },
    {
      "name": "<git-subcommand>",
      "subdesc": "Execute the subcommand for repositories.",
      "usages": [
        "%s [<options>] %s [<args>]",
        "%s %s (--version | --help)"
      ],
      "options": [
      ]
    }
  ]
}
